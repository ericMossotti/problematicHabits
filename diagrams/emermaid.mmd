    PARTICIPANT ||--o{ DEMOGRAPHICS : has
    PARTICIPANT ||--o{ PHYSICAL_MEASURES : has
    PARTICIPANT ||--o{ FITNESSGRAM : has
    PARTICIPANT ||--o{ BIA : has
